<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <pipelines>
      <initialize>
        <processor type="Helixbase.Feature.Analytics.Routes.RegisterRoutes, Helixbase.Feature.Analytics"
                   patch:before="processor[@type='Sitecore.Mvc.Pipelines.Loader.InitializeRoutes, Sitecore.Mvc']" resolve="true"  />
      </initialize>
    </pipelines>
    <settings>

      <setting name="Sitecore.Services.AllowAnonymousUser">
        <patch:attribute name="value">true</patch:attribute>
      </setting>
 

    </settings>

 
  </sitecore>
</configuration>